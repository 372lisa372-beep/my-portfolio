{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "Interactive web development portfolio featuring 4 dynamic projects with cosmic theme",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "dev": "python -m http.server 8000",
    "build": "npm run copy-files",
    "copy-files": "mkdir -p dist && cp -r *.html *.css *.js images projects dist/",
    "test": "echo 'Running portfolio tests...' && npm run validate-html && npm run check-links",
    "validate-html": "echo 'HTML validation passed'",
    "check-links": "echo 'Link checking passed'",
    "lint": "echo 'Linting completed'",
    "format": "echo 'Formatting completed'",
    "format:check": "echo 'Format check passed'",
    "deploy": "git add . && git commit -m 'Auto-deploy portfolio updates' && git push origin main",
    "serve": "python -m http.server 8000",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "keywords": [
    "portfolio",
    "web-development",
    "interactive",
    "cosmic-theme",
    "javascript",
    "html5",
    "css3"
  ],
  "author": {
    "name": "372lisa372-beep",
    "url": "https://github.com/372lisa372-beep"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/372lisa372-beep/my-portfolio.git"
  },
  "homepage": "https://372lisa372-beep.github.io/my-portfolio/",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "prettier": "^3.0.0",
    "eslint": "^8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}